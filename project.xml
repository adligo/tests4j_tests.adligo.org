<?xml version="1.0" encoding="UTF-8"?>
<tns:fabricate_project 
	xmlns:cns="http://www.adligo.org/fabricate/xml/io_v1/common_v1_0.xsd" 
	xmlns:tns="http://www.adligo.org/fabricate/xml/io_v1/project_v1_0.xsd" 
    xmlns:lns="http://www.adligo.org/fabricate/xml/io_v1/library_v1_0.xsd" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
    <tns:stages>
        <tns:stage name="jar" >
        	<tns:params>
            	<cns:param key="srcDirs" value="src,src_jdk_1_8" />
            </tns:params>
        </tns:stage>
        <tns:stage name="test" />
        <tns:stage name="publish" />
    </tns:stages>
    
    
    <tns:dependencies>
        <lns:library>ide</lns:library>
        <lns:library>jacoco</lns:library>
        <lns:library>mockito</lns:library>
        <lns:dependency group="java" artifact="java" version="default" type="ide">
	    	<lns:ide name="eclipse" >
	    		<lns:arg key="src" value="src,src_jdk_1_8" />
	    	</lns:ide>
	    </lns:dependency>
        <lns:project>gwt_refs.adligo.org</lns:project>
        <lns:project>tests4j.adligo.org</lns:project>
        <lns:project>tests4j_4jacoco.adligo.org</lns:project>
        <lns:project>tests4j_4mockito.adligo.org</lns:project>
       
    </tns:dependencies>
 </tns:fabricate_project>